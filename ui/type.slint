// ui 数据类型定义

// 歌曲展示标题
export struct ItemTitleInfo {
    text: string, // 文本内容
    width: percent, // 占用宽度百分比
}

// 歌曲元数据信息
export struct MusicInfo {
    path: string, // 歌曲绝对路径
    title: string, // 曲名
    artist: string, // 艺术家这里仅仅是歌唱者
    album: string, // 专辑
    duration: float, // 时长
}

// 歌词
export struct LyricInfo {
    time: float, // 歌词秒数
    text: string, // 歌词文本
}
